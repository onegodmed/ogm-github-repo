<script>
var section_number = 0;
function call_section(section_num) {
    $("#modal-body").html("<img src='<?= base_url() ?>assets/img/load-icon.gif' style='display: block;margin-left: auto;margin-right: auto'>");
    $("#msg").html('');
    section_number = section_num;
    $.ajax({
        url: "<?= base_url('developer/edit_profile/' . $user[0]->user_id . '/') ?>" + section_number,
        success: function (result) {
            $("#modal-body").html(result);
        }});
}
</script>
<script>
//submit edit form
//$(document).ready(function(){
$("#edit_submit").click(function (event) {
    /* $("#edit_submit").html("<img src='<?= base_url() ?>assets/img/load-icon.gif' style='display: block;margin-left: auto;margin-right: auto;width: 50%;'>");*/
    $("#edit_submit").html("loading...");

//to notify user the file is being uploaded
    files = event.target.files;
// get the selected files
    var data = new FormData($('#edit_form')[0]);
// Form Data check the above bullet for what it is  
    var error = 0;
// Flag to notify in case of error and abort the upload

// File data is presented as an array. In this case we can just jump to the index file using files[0] but this array traversal is recommended
    var files = $("input[type='file']");
    for (var i = 0; i < files.length; i++) {

        var file = files[i];
        if (!file.type.match('image.*')) {
// Check for File type. the 'type' property is a string, it facilitates usage if match() function to do the matching                 

        } else if (file.size > 1048576) {
// File size is provided in bytes                
            alert("file is too large");
            error = 1;
        } else {
//        alert(files[i].name);
// If all goes well, append the up-loadable file to FormData object
            data.append('image', file, file.name);
// Comparing it to a standard form submission the 'image' will be name of input
        }
    }

    if (error == 0) {
//        $("#edit_submit").attr("disabled", "disabled");
        $.ajax({
            url: "<?= base_url('developer/edit_profile/' . $user[0]->user_id . '/') ?>" + section_number,
            type: 'post',
            enctype: 'multipart/form-data',
            dataType: 'json',
//            data: $("#edit_form").serialize(),
            data: data,
            async: false,
            cache: false,
            contentType: false,
            processData: false,

            success: function (response) {

//                alert(response.status);
                if (response.status == 'success') {
//                        alert('asddsf');
//                        $('#msg').html('<div class="alert alert-success alert-dismissible fade in"><a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a><strong>Success!</strong> ' + response.msg + '</div>');
//                        $('.embedModal').modal('hide');                        
//                        $("#edit_submit").html("Submit");
//                        $("#edit_submit").attr("disabled", false);
                    window.location.href = response.url;                        
                    window.location.href = response.url;
                } else {
                    $('#msg').html('<div class="alert alert-danger alert-dismissible fade in"><a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a><strong>Error!</strong> ' + response.msg + '</div>');
                    $("#edit_submit").html("Submit");
                    $("#edit_submit").attr("disabled", false);
//                    if (response.url !== '')
//                        window.location.href = response.url;
                    return false;
                }
                $("#edit_submit").attr("disabled", false);
            }
        });
    }

});
//});
</script>
