<script>
$(function () {
    $('.askbtn').click(function () {
        if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
            var target = $(this.hash);
            target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
            if (target.length) {
                $('html, body').animate({
                    scrollTop: target.offset().top
                }, 3000);
                return false;
            }
        }
    });
});
function full_time() {
    $("#job_type_headig").html('<i class="ti-clock-pin padd-r-10"></i>Full Titme');
    $("#rate").html('<i class="ti-credit-card padd-r-10"></i>Hourly : <?php echo $user[0]->full_time_amount ?>$/hr');
    $("#end_date_row").show();
    $("#start_date_row").hide();
    $("#hours_row").hide();
    $("#amount_row").hide();
    $("#job_type_val").val('full time');
}
function part_time() {
    $("#job_type_headig").html('<i class="ti-clock-pin padd-r-10"></i>Part Titme');
    $("#end_date_row").show();
    $("#start_date_row").hide();
    $("#hours_row").hide();
    $("#amount_row").hide();
    $("#rate").html('<i class="ti-credit-card padd-r-10"></i>Hourly : <?php echo $user[0]->part_time_amount ?>$/hr');
    $("#job_type_val").val('part time');
}
function contract() {
    $("#job_type_val").val('contract');
    $("#start_date_row").hide();
    $("#end_date_row").hide();
    $("#hours_row").show();
    $("#amount_row").show();
    $("#job_type_headig").html('<i class="ti-clock-pin padd-r-10"></i>Contract');
    $("#rate").html('<i class="ti-credit-card padd-r-10"></i>Contract : <?php echo $user[0]->contract_amount ?>$');
}

function form_submit() {
    var job_type = $("#job_type_val").val();
    var submit_status = false;
    if (job_type.toLowerCase() === 'full time') {
        if ($("#end_date").val() !== '')
            $("#hire_form").submit();
        else
            swal("Error!", "Please Select End Date.", "error");
    } else if (job_type.toLowerCase() === 'part time') {
        if ($("#end_date").val() !== '')
            $("#hire_form").submit();
        else
            swal("Error!", "Please Select End Date.", "error");
    } else if (job_type.toLowerCase() === 'contract') {
        if ($("#contact_hours").val() !== '' && $("#contact_amount").val() !== '')
            $("#hire_form").submit();
        else
            swal("Error!", "Please Select Contract Hours.", "error");
    }

}

$("#contact_hours").on('change', function () {
    var hours = parseInt($(this).val());
    var amount = hours *<?= $user[0]->contract_amount; ?>;
    $("#contract_amount").val(amount);
});

$('#start_date').datepicker({}).on('changeDate', function (selected) {
    startDate = new Date(selected.date.valueOf());
    startDate.setDate(startDate.getDate(new Date(selected.date.valueOf())));
    $('#end_date').datepicker('setStartDate', startDate);
});


$('#end_date').datepicker({}).on('changeDate', function (selected) {
    FromEndDate = new Date(selected.date.valueOf());
    FromEndDate.setDate(FromEndDate.getDate(new Date(selected.date.valueOf())));
    $('#start_date').datepicker('setEndDate', FromEndDate);
});

$(document).ready(function () {
<?php
if (strcasecmp($job_type, 'full time') == 0)
echo "full_time();";
if (strcasecmp($job_type, 'part time') == 0)
echo "part_time();";
if (strcasecmp($job_type, 'contract') == 0)
echo "contract();";
?>



});


</script> 


